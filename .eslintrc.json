{
  "extends": [
    "prettier"
  ],
  "plugins": [
    "prettier",
    "dollar-sign",
    "jquery"
  ],
  "rules": {
    "prettier/prettier": ["error"]
  },
  "parserOptions": {
    "ecmaVersion": "latest"
  },
  "env": {
    "browser": true,
    "commonjs": true,
    "es6": true,
    "jquery": true
  }
}
